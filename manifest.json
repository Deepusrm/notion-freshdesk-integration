{
  "platform-version": "2.3",
  "product": {
    "freshdesk": {
      "events": {
        "onConversationCreate": {
          "handler": "onConversationCreateHandler"
        }
      },
      "requests": {
        "onGettingPages": {
          "schema": {
            "method": "GET",
            "host": "api.notion.com",
            "path": "/v1/pages/<%=iparam.notion_page %>",
            "headers": {
              "Authorisation": "Bearer <%=iparam.notion_api_key %>",
              "Content-Type": "application/json"
            }
          }
        },
        "onCreatingPrivateNote": {
          "schema": {
            "method": "POST",
            "host": "api.notion.com",
            "path": "/v1/pages",
            "headers": {
              "Authorisation": "Bearer <%=iparam.notion_api_key %>",
              "Content-Type": "application/json"
            }
          }
        }
      }
    }
  },
  "engines": {
    "node": "18.19.0",
    "fdk": "9.1.0"
  }
}